{
  "agent": {
    "always_allow_tool_actions": true,
    "default_model": {
      "model": "claude-sonnet-4",
      "provider": "zed.dev"
    },
    "preferred_completion_mode": "max",
    "version": "2"
  },
  "base_keymap": "VSCode",
  "buffer_font_family": "Fira Code Retina",
  "buffer_font_features": {
    "ss09": true
  },
  "buffer_font_size": 14,
  "completions": {
    "words": "disabled"
  },
  "edit_predictions": {
    "disabled_globs": [
      "*.md"
    ],
    "mode": "subtle"
  },
  "features": {
    "edit_prediction_provider": "zed"
  },
  "format_on_save": "off",
  "git": {
    "inline_blame": {
      "enabled": false
    }
  },
  "inlay_hints": {
    "enabled": true
  },
  "languages": {
    "Gleam": {
      "language_servers": [
        "gleam",
        "..."
      ]
    },
    "Nickel": {
      "language_servers": [
        "nls",
        "..."
      ],
      "tab_size": 2
    },
    "Nix": {
      "language_servers": [
        "!nil",
        "!nixd",
        "..."
      ],
      "tab_size": 2
    },
    "Rust": {
      "preferred_line_length": 120
    },
    "SCSS": {
      "tab_size": 2
    },
    "SQL": {
      "prettier": {
        "allowed": false
      }
    },
    "WIT": {
      "language_servers": [
        "!wit",
        "..."
      ]
    }
  },
  "lsp": {
    "rust-analyzer": {
      "initialization_options": {
        "imports": {
          "granularity": {
            "enforce": true,
            "group": "module"
          },
          "prefix": "crate"
        }
      }
    }
  },
  "project_panel": {
    "auto_fold_dirs": false
  },
  "seed_search_query_from_cursor": "selection",
  "show_whitespaces": "boundary",
  "tabs": {
    "file_icons": true
  },
  "theme": "Melange Dark",
  "ui_font_family": "Zed Plex Sans",
  "ui_font_size": 16,
  "when_closing_with_no_tabs": "keep_window_open"
}
